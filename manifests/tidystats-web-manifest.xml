<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>f2753d41-d72a-4c16-8836-192052c7f43b</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>WillemSleegers</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="tidystats"/>
  <Description DefaultValue="Reproducibly report statistics."/>
  <IconUrl DefaultValue="https://tidystats-word.uvt.nl/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://tidystats-word.uvt.nl/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://willemsleegers.github.io/tidystats-Word-add-in/"/>
  <AppDomains>
    <AppDomain>https://tidystats-word.uvt.nl</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <Requirements>
   <Sets DefaultMinVersion="1.1">
      <Set Name="WordApi" MinVersion="1.1"/>
   </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://tidystats-word.uvt.nl/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabInsert">
              <Group id="Group">
                <Label resid="Group.Label" />
                <Icon>
                  <bt:Image size="16" resid="icon-16x16" />
                  <bt:Image size="32" resid="icon-32x32" />
                  <bt:Image size="40" resid="icon-40x40" />
                  <bt:Image size="48" resid="icon-48x48" />
                  <bt:Image size="64" resid="icon-64x64" />
                  <bt:Image size="80" resid="icon-80x80" />
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label" />
                  <Supertip>
                    <Title resid="TaskpaneButton.Label" />
                    <Description resid="TaskpaneButton.Tooltip" />
                  </Supertip>
                  <Icon>
                  <bt:Image size="16" resid="icon-16x16" />
                  <bt:Image size="32" resid="icon-32x32" />
                  <bt:Image size="40" resid="icon-40x40" />
                  <bt:Image size="48" resid="icon-48x48" />
                  <bt:Image size="64" resid="icon-64x64" />
                  <bt:Image size="80" resid="icon-80x80" />
                </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ShowTaskpaneButton</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="icon-16x16" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-16.png"/>
        <bt:Image id="icon-32x32" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-32.png"/>
        <bt:Image id="icon-40x40" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-40.png"/>
        <bt:Image id="icon-48x48" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-48.png"/>
        <bt:Image id="icon-64x64" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-64.png"/>
        <bt:Image id="icon-80x80" DefaultValue="https://tidystats-word.uvt.nl/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://willemsleegers.github.io/tidystats-Word-add-in/" />
        <bt:Url id="Taskpane.Url" DefaultValue="https://tidystats-word.uvt.nl/taskpane.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with tidystats!" />
        <bt:String id="Group.Label" DefaultValue="Commands Group" />
        <bt:String id="TaskpaneButton.Label" DefaultValue="Insert Statistics" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="tidystats loaded succesfully. Go to the INSERT tab and click the 'Insert Statistics' button to get started." />
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to open tidystats" />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>